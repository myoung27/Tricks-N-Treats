<h1>Review a House</h1>

<div class="card" style="width: 18rem;">
    {{#each houses}}
    <div class="card-body">
        <h5 class="card-title">{{address}}</h5>
        <a href="/neighborhoods/{{_id}}/houses/{{_id}}" class="btn btn-primary">See this House</a>
    </div>
</div>
{{/each}}
<div>
    <a href="/neighborhoods/{{ _id }}/houses/new"><button type="button" class="btn btn-primary">Create A New House</button></a>